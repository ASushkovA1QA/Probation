<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <appSettings>
    <add key="browser" value="chrome" />
    <add key="remote" value="false" />
    <add key="grid.hub" value="http://192.168.16.10:5558/wd/hub/" />
    <add key="MobileTesting" value="false" />
    <add key="BrowserLang" value="en" />
    <add key="StartProxyServer" value="false" />
    <add key="EdgeDriverPath" value="false" />
    <add key="browserstack.emulator" value="false"/>
    <add key="ImageFormat" value="png"/>
    <add key="element_timeout" value="80" />
    <add key="page_timeout" value="80" />
    <add key="email_wait_timeout" value="120" />
    <add key="platform.vesion" value="php" />
  </appSettings>
  <log4net debug="false">
    <appender name="Console" type="log4net.Appender.ConsoleAppender">
      <!-- A1 uses PatternLayout -->
      <layout type="log4net.Layout.PatternLayout">
        <!-- Print the date in ISO 8601 format -->
        <conversionPattern value="%date %-5level  - %message%newline" />
      </layout>
    </appender>
    <appender name="fileApp" type="log4net.Appender.FileAppender">
      <param name="File" value="LogFile.txt" />
      <param name="AppendToFile" value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <!-- Print the date in ISO 8601 format -->
        <conversionPattern value="%message%newline" />
      </layout>
    </appender>
    <root>
      <appender-ref ref="Console" />
      "
      <appender-ref ref="fileApp" />"
    </root>
  </log4net>
  </configuration>